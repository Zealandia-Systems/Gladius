.overrides {
    margin-bottom: 10px;

    .dro {
        margin-bottom: 10px;
    }

    .dro-label,
    .dro-display,
    .dro-btn-group {
        height: 30px;
    }
    .dro-label {
        font-size: 20px;
        line-height: initial;
        text-align: left;
    }
    .dro-display {
        padding: 4px 6px;
        text-align: right;
        font-size: 14px;
        text-overflow: initial;
        margin-right: 5px;
    }
    .dro-btn-group {
        margin-left: 5px;
    }
    .dro-btn {
        padding: 5px 5px;
    }
}

.well {
    min-height: 22px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    background-color: #f5f5f5;
    border: 1px solid #e3e3e3;
    border-radius: 3px;
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);
    margin-bottom: 0;
    padding: 0 5px;
}

.divider {
    margin: 0px;
}
